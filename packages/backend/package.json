{
    "name": "wt2backend",
    "version": "0.0.1",
    "description": "Awesome project developed with TypeORM.",
    "type": "commonjs",
    "devDependencies": {
        "@types/cors": "^2.8.13",
        "@types/debug": "^4.1.7",
        "@types/express": "^4.17.16",
        "@types/express-fileupload": "^1.4.0",
        "@types/helmet": "^0.0.43",
        "@types/node": "^18.11.18",
        "@types/sharp": "^0.31.1",
        "@types/uniqid": "^5.3.2",
        "nodemon": "^2.0.20",
        "ts-node": "^10.9.1",
        "tslint": "^5.14.0",
        "typescript": "^4.9.4"
    },
    "dependencies": {
        "bcryptjs": "^2.4.3",
        "body-parser": "^1.20.1",
        "class-validator": "^0.14.0",
        "cors": "^2.8.5",
        "express": "^4.18.2",
        "express-fileupload": "^1.4.0",
        "helmet": "^6.0.1",
        "http-status-codes": "^2.2.0",
        "jsonwebtoken": "^9.0.0",
        "pg": "^8.4.0",
        "reflect-metadata": "^0.1.13",
        "sharp": "^0.34.2",
        "typeorm": "0.3.11",
        "uniqid": "^5.4.0"
    },
    "scripts": {
        "build": "tsc",
        "clean": "rm -rf build",
        "start": "DEBUG=api:* node build/Server.js",
        "dev": "DEBUG=api:* node --inspect -r ts-node/register src/Server.ts",
        "watch": "nodemon",
        "migrate": "ts-node ../../node_modules/typeorm/cli.js migration:run"
    },
    "nodemonConfig": {
        "ignore": [
            "**/*.test.ts",
            ".git",
            "node_modules"
        ],
        "watch": [
            "src",
            ".env"
        ],
        "exec": "yarn dev",
        "ext": "ts",
        "inspect": true
    }
}
